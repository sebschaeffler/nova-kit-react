{
  "name": "nova-kit-react",
  "private": false,
  "version": "0.3.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sebschaeffler/nova-kit-react.git"
  },
  "author": "Sebastien Schaeffler",
  "license": "MIT",
  "keywords": [
    "react",
    "storybook",
    "ui-components",
    "typescript",
    "tailwindcss"
  ],
  "homepage": "https://github.com/sebschaeffler/nova-kit-react#readme",
  "bugs": {
    "url": "https://github.com/sebschaeffler/nova-kit-react/issues"
  },
  "contributors": [
    {
      "name": "Sebastien Schaeffler",
      "email": "sebastien@schaeffler.dev",
      "url": "https://github.com/sebschaeffler"
    }
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.es.js",
      "require": "./dist/components.cjs.js"
    },
    "./lib": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.es.js",
      "require": "./dist/lib.cjs.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.es.js",
      "require": "./dist/hooks.cjs.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.cjs.js"
    },
    "./nova-styles.css": "./dist/nova-styles.css",
    "./tailwind.config.ts": "./dist/tailwind.config.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm build-tsup",
    "build-vite": "tsc && vite build && npm run build:styles",
    "build:styles": "postcss src/nova-styles.css -o ./dist/nova-styles.css",
    "build-tsup": "tsup && npm run build:styles",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "lucide-react": "^0",
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@storybook/blocks": "^8.6.6",
    "@tanstack/react-query": "^5.56.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^1.2.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.5.2",
    "next-themes": "^0.4.4",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@faker-js/faker": "^9.6.0",
    "@storybook/addon-docs": "^8.6.6",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.6.6",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.6.6",
    "@storybook/test": "^8.1.11",
    "@tanstack/react-table": "^8.21.2",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^2.3.0"
  }
}
